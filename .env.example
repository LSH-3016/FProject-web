# FastAPI Backend URL (Library API)
VITE_LIBRARY_API_URL=http://192.168.1.100:8000/api/v1

# Journal API URL (History도 이 API를 사용)
VITE_JOURNAL_API_URL=http://localhost:8000

# Image Generator API URL (AI 이미지 생성)
VITE_IMAGE_API_URL=https://image.aws11.shop

# Cognito API URL (사용자 관리 API)
VITE_COGNITO_API_URL=http://localhost:3001

# AWS Cognito Configuration
VITE_COGNITO_REGION=ap-northeast-2
VITE_COGNITO_USER_POOL_ID=ap-northeast-2_XXXXXXXXX
VITE_COGNITO_CLIENT_ID=your_client_id_here
VITE_COGNITO_DOMAIN=your-domain.auth.ap-northeast-2.amazoncognito.com

# OAuth Configuration
VITE_OAUTH_REDIRECT_URI=http://localhost:5173/auth/callback

# AWS Configuration (for server-side operations)
VITE_AWS_REGION=ap-northeast-2
VITE_AWS_USER_POOL_ID=ap-northeast-2_XXXXXXXXX
VITE_AWS_USER_POOL_WEB_CLIENT_ID=your_client_id_here
VITE_AWS_COGNITO_DOMAIN=https://your-domain.auth.ap-northeast-2.amazoncognito.com

# Google OAuth Client ID (예시 - 실제 값으로 교체 필요)
VITE_GOOGLE_CLIENT_ID=123456789012-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com